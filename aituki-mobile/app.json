{
  "expo": {
    "name": "AiTuki",
    "slug": "aituki-native",
    "version": "2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aitukinative",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aituki.mobile",
      "buildNumber": "20",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSHealthShareUsageDescription": "AiTuki needs access to your health data to provide personalized insights and track your wellbeing across physical, emotional, mental, and energy pillars.",
        "NSHealthUpdateUsageDescription": "AiTuki needs to write health data to help you track your progress and goals."
      }
    },
    "android": {
      "package": "com.aituki.mobile",
      "versionCode": 17,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#4EC9CD"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them.",
          "cameraPermission": "The app accesses your camera to let you take photos."
        }
      ],
      [
        "@kingstinct/react-native-healthkit",
        {
          "healthSharePermission": "AiTuki needs access to your health data to provide personalized insights and track your wellbeing across physical, emotional, mental, and energy pillars.",
          "healthUpdatePermission": "AiTuki needs to write health data to help you track your progress and goals."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
